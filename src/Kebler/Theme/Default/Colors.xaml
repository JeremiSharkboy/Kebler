    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Kebler.Theme.Default">
    <Color
        x:Key="TransparentColor"
        A="0"
        B="0"
        G="0"
        R="0" />
    <Color
        x:Key="DarkRedColor"
        A="0"
        B="96"
        G="102"
        R="255" />


