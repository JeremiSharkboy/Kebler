<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <AssemblyName>Kebler</AssemblyName>
    <LangVersion>8.0</LangVersion>
    <Company />
    <PackageIconUrl></PackageIconUrl>
    <ApplicationIcon>Theme\Icons\Kebler.ico</ApplicationIcon>
    <NoWin32Manifest>true</NoWin32Manifest>
    <StartupObject>Kebler.App</StartupObject>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <GenerateSerializationAssemblies>Off</GenerateSerializationAssemblies>
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Theme\Icons\Kebler.svg" />
    <None Remove="Theme\Icons\Kebler_16x16.png" />
    <None Remove="Theme\Icons\Kebler_32x32.png" />
    <None Remove="Theme\Icons\Kebler_512x512.png" />
    <None Remove="Theme\Icons\Kebler_64x64.png" />
    <None Remove="Theme\Icons\Kebler_dark_512x512.png" />
    <None Remove="Theme\Icons\Kebler_darl2_512x512.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="LiteDB" Version="4.1.4" />
    <PackageReference Include="log4net" Version="2.0.8" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="PropertyChanged.Fody" Version="2.6.0" />
    <PackageReference Include="sharpconfig" Version="3.2.8" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Transmission.API.RPC-master\Transmission.API.RPC\Transmission.API.RPC.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Theme\Icons\Kebler.ico" />
    <Resource Include="Theme\Icons\Kebler.svg" />
    <Resource Include="Theme\Icons\Kebler_16x16.png" />
    <Resource Include="Theme\Icons\Kebler_32x32.png" />
    <Resource Include="Theme\Icons\Kebler_512x512.png" />
    <Resource Include="Theme\Icons\Kebler_64x64.png" />
    <Resource Include="Theme\Icons\Kebler_dark_512x512.png" />
    <Resource Include="Theme\Icons\Kebler_darl2_512x512.png" />
  </ItemGroup>
  <ItemGroup>
    <None Update="log4net.config">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>