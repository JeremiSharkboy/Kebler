    <sys:String x:Key="ConnectionManagerTitle">Connection Manager</sys:String>
    <sys:String x:Key="GeneralTabTitle">General</sys:String>
    <sys:String x:Key="ProxyTabTitle">Proxy</sys:String>

    <sys:String x:Key="AddBtnContent">+</sys:String>
    <sys:String x:Key="RemoveBtnContent">-</sys:String>

    <sys:String x:Key="ServerTitleLblContent">Connection title:</sys:String>
    <sys:String x:Key="RemoteHostLblContent">Remote Host:</sys:String>
    <sys:String x:Key="RemotPortLblContent">Remote Port:</sys:String>
    <sys:String x:Key="IsNeedAuthLblContent">Authentication required</sys:String>
