    <sys:String x:Key="DialogEnterPasswordContent">Please enter password:</sys:String>
    <sys:String x:Key="DialogOkButtonContent">Ok</sys:String>

    <!--  MainWindow  -->
    <sys:String x:Key="NotYetConnectedContent">Not Connected To Server:</sys:String>
    <sys:String x:Key="NoAnyTorrents">No Any Torrents</sys:String>
